{
  "name": "ai-pdf-viewer",
  "private": false,
  "version": "0.1.0",
  "description": "AI-powered PDF viewer with synchronized reading, pronunciation checking, and intelligent document analysis",
  "author": "AI PDF Viewer Contributors",
  "license": "(MIT OR Apache-2.0)",
  "repository": {
    "type": "git",
    "url": "https://github.com/arjun-veer/ai-pdf-viewer.git"
  },
  "bugs": {
    "url": "https://github.com/arjun-veer/ai-pdf-viewer/issues"
  },
  "keywords": [
    "pdf",
    "viewer",
    "ai",
    "tauri",
    "react",
    "typescript",
    "tts",
    "ocr",
    "translation",
    "pronunciation"
  ],
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "tauri": "tauri",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "format": "prettier --write \"src/**/*.{ts,tsx,css}\"",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage"
  },

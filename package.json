{
  "name": "ai-pdf-viewer",
  "private": false,
  "version": "0.0.1",
  "description": "AI-powered PDF viewer with synchronized reading, pronunciation checking, and intelligent document analysis",
  "author": "AI PDF Viewer Contributors",
  "license": "(MIT OR Apache-2.0)",
  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/ai-pdf-viewer.git"
  },
  "bugs": {
    "url": "https://github.com/yourusername/ai-pdf-viewer/issues"
  },
  "keywords": [
    "pdf",
    "viewer",
    "ai",
    "tauri",
    "react",
    "typescript",
    "tts",
    "ocr",
    "translation",
    "pronunciation"
  ],
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "tauri": "tauri",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "format": "prettier --write \"src/**/*.{ts,tsx,css}\""
  },
  "dependencies": {
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "@tauri-apps/api": "^2",
    "@tauri-apps/plugin-opener": "^2"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4.1.18",
    "@tauri-apps/cli": "^2",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@vitejs/plugin-react": "^4.6.0",
    "autoprefixer": "^10.4.23",
    "postcss": "^8.5.6",
    "tailwindcss": "^4.1.18",
    "typescript": "~5.8.3",
    "vite": "^7.0.4"
  }
